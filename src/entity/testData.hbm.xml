<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entity.TestData" table="TestData">
        <id name="id" type="java.lang.String" length="16">
            <generator class="assigned"></generator>
        </id>
        <property name="pid" type="java.lang.String"/>
        <property name="input" type="java.lang.String"/>
        <property name="output" type="java.lang.String"/>

    </class>
</hibernate-mapping>